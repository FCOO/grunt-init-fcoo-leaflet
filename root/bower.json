{
  "name"		: "{%= name %}",
  "version"	: "0.0.0",
  "homepage": "https://github.com/FCOO/{%= name %}",
  "authors": [
    "{%= author_name %} {%= author_email %}"
  ],
  "description": "{%= description %}",
  "main": [
    "dist/{%= name %}.min.js",
    "dist/{%= name %}.min.css"
  ],
	"repository": {
		"type": "git",
		"url": "git://github.com/FCOO/{%= name %}.git"
	},

	"dependencies": {
    "leaflet": "latest"
	},

	"devDependencies": {
    "bourbon": "latest",
		"modernizr-mixin": "latest",
		"mathsass": "latest"
  },
	
	"license": "MIT",

  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "moduleType": [
    "es6"
  ]
}
